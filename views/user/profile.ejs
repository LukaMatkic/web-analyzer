<!-- Title -->
<div id="titleDiv2">
	<center>
		<p>User account</p>
	</center>
</div>

<div id="snatcherDiv2">
	<center>
		<p>PROFILE</p>
	</center>
</div>

<hr style="newstyle">

<!--If there is sucess we display it -->
<% if(typeof sucess != 'undefined') { %>
	<% if(sucess.length > 0) { %>
  <div class="sucmsg">
		<%= sucess %>
	</div>
<% } %>
<% } %>


<!--If there is error we display it -->
<% if(typeof error != 'undefined') { %>
	<% if(error.length > 0) { %>
	  <div class="errmsg">
			<%= error %>
		</div>
	<% } %>
<% } %>


<div class="content-div">
	<a href="/logout">Logout</a>
</div>

<div class="content-div">
	<p>
		<strong>id</strong>: <%= user.id %><br>
		<strong>username</strong>: <%= user.username %><br>
		<strong>password</strong>: <%= user.password %>
	</p>
</div>

<hr style="newstyle">

<div class="content-div">
	<p>Want to scrape some site ? Click <a href="/scraper">here</a>.</p>
</div>
