<div class="content-wrapper">
<div id="titleDiv">
		Statistic tools
</div>

<div id="snatcherDiv">
		<p>
		SITE DATA
		</p>
		</div>


<!--If there is error we display it -->
<% if(typeof sucess != 'undefined') { %>
  <div class="sucmsg">
    <p><%= sucess %></p>
  </div>
<% } %>


<!--If there is error we display it -->
<% if(typeof error != 'undefined') { %>
  <div class="errmsg">
    <p><%= error %></p>
  </div>
<% } %>


<!-- If there is warning we display it -->
<% if(typeof warn != 'undefined') { %>
  <div class="wrnmsg">
    <p><%= warn %></p>
  </div>
<% } %>


<!--If there is warning we display it -->
<% if(typeof info != 'undefined') { %>
  <div class="infmsg">
    <p><%= info %></p>
  </div>
<% } %>

<!-- Ako samo prikazujemo stranicu  -->
<% if(typeof analyzed != 'undefined') { %>
	<div id="pageTextContent">
	<center>
	<%= analyzed[0].title %>
	</center>
</div>


<section>

<div class="tbl-header">
		<table cellpadding="0" cellspacing="0" border="0">
				<thead>
				<tr>
					<th colspan="5"> GENERAL DATA</th>
				</tr>
						<tr>
						<th>URL</th>
						<th>TITLE</th>
						<th>ANALYZED DATE</th>
						<th>ANALYZED TIME</th>
						<th>SCRAP ID</th>
						</tr>
				</thead>
		</table>
	</div>
<div class="tbl-content">
		<table>
				<tbody>

					<tr>
					<td data-th="URL"><center><%=analyzed[0].url%>
					</center></td>
					<td data-th="TITLE"><%=analyzed[0].title%></td>
					<td data-th="ANALYZED DATE"><%= analyzed[0].date.getDate() + '.' + parseInt(analyzed[0].date.getMonth()+1) + '.' + analyzed[0].date.getFullYear() + '.' %></td>
					<td data-th="ANALYZED TIME"><%=analyzed[0].time%></td>
					<td data-th="SCRAP ID"><%=analyzed[0].id%></td>
					</tr>

				</tbody>

		</table>
</div>

</section>


<section>

<div class="tbl-header">
		<table cellpadding="0" cellspacing="0" border="0">
				<thead>
				<tr>
					<th colspan="4"> HTTP HEADERS</th>
				</tr>
						<tr>
						<th>HTTP STATUS</th>
						<th>CONTENT LENGTH</th>
						<th>CONTENT TYPE</th>
						<th>SERVER</th>
						</tr>
				</thead>
		</table>
	</div>
<div class="tbl-content">
		<table>
				<tbody>

					<tr>
					<td data-th="HTTP STATUS"><%=analyzed[0].http_status%></td>
					<td data-th="CONTENT LENGTH"><%=analyzed[0].con_length%></td>
					<td data-th="CONTENT TYPE"><%=analyzed[0].con_type%></td>
					<td data-th="SERVER"><%=analyzed[0].server%></td>
					</tr>

				</tbody>

		</table>
</div>

</section>

<section>

<div class="tbl-header">
		<table cellpadding="0" cellspacing="0" border="0">
				<thead>
				<tr>
					<th colspan="2"> HTML TAGS</th>
				</tr>
						<tr>
						<th>TITLE</th>
						<th>CHARSET</th>
						</tr>
				</thead>
		</table>
	</div>
<div class="tbl-content">
		<table>
				<tbody>

					<tr>
					<td data-th="TITLE"><%=analyzed[0].title%></td>
					<td data-th="CHARSET"><%=analyzed[0].charset%></td>
					</tr>

				</tbody>

		</table>
</div>

</section>






        <section>
        <% if(headers != '') { %>


					<div class="tbl-header">
							<table>
									<thead>
									<tr>
										<th colspan="2">HEADERS</th>
									</tr>
											<tr>
											<th>"'be'"</Tag></th>
											<th>VALUE</th>
											</tr>
									</thead>
							</table>
						</div>

						<div class="tbl-content">
								<table>
										<tbody>

										<tbody>
										<!-- Za svaki ispis iz tablice stvaramo row u tablici -->
										<% for(var i=0;i<headers.length;i++) {%>

											<!-- Dodajemo razmak prema value-u headera -->
											<% if(headers[i].head_value == 0) { %>
												<tr class="table-body h1-style">
													<td style="color: blue"><%= headers2[i] %></td>
													<td style="text-indent: 5px"><%= headers[i].head_text %></td>
												</tr>
											<% } else if(headers[i].head_value == 1) { %>
												<tr class="table-body h2-style">
													<td style="color: blue"><%= headers2[i] %></td>
													<td style="text-indent: 10px"><%= headers[i].head_text %></td>
												</tr>
											<% } else if(headers[i].head_value == 2) { %>
												<tr class="table-body h3-style">
													<td style="color: blue"><%= headers2[i] %></td>
													<td style="text-indent: 15px"><%= headers[i].head_text %></td>
												</tr>
											<% } else if(headers[i].head_value == 3) { %>
												<tr class="h4-style">
													<td style="color: blue"><%= headers2[i] %></td>
													<td style="text-indent: 20px"><%= headers[i].head_text %></td>
												</tr>
											<% } else if(headers[i].head_value == 4) { %>
												<tr class="h5-style">
													<td style="color: blue"><%= headers2[i] %></td>
													<td style="text-indent: 25px"><%= headers[i].head_text %></td>
												</tr>
											<% } else if(headers[i].head_value == 5) { %>
												<tr class="h6-style">
													<td style="color: blue"><%= headers2[i] %></td>
													<td style="text-indent: 30px"><%= headers[i].head_text %></td>
												</tr>
											<% } else if(headers[i].head_value == 6) { %>
												<% if(headers[i].head_text.length > 2) { %>
													<tr class="p-style">
														<td style="color: blue"><%= headers2[i] %></td>
														<td style="text-indent: 35px"><%= headers[i].head_text %></td>
													</tr>
												<% } %>
											<% } else if(headers[i].head_value == 7) { %>
												<% if(headers[i].head_text.length > 0) { %>
													<tr class="a-style">
														<td style="color: blue"><%= headers2[i] %></td>
															<td style="text-indent: 40px">
																<% if(headers[i+1].head_text.length > 2) { %>
																	<% if(headers[i+1].head_text[0] != '/') { %>
																		<% if(headers[i+1].head_text[0] != '#') { %>
																			<% if(headers[i+1].head_text[0] != '_') { %>
																				<a target="_blank" href="<%= headers[i+1].head_text %>" class="a-style">
																					<%= headers[i].head_text %>
																				</a>
																			<% } else { %>
																				<%= headers[i].head_text %>
																			<% } %>
																		<% } else { %>
																			<%= headers[i].head_text %>
																		<% } %>
																	<% } else { %>
																		<%= headers[i].head_text %>
																	<% } %>
																<% } else { %>
																	<%= headers[i].head_text %>
																<% } %>
															</td>
													</tr>
												<% }%>
											<% } else if(headers[i].head_value == 8) { %>
											<% }%>
										</tbody>
										<% }%>
									</table>
					<% }%>

										</tbody>

								</table>
						</div>


        </section>





		<!-- Sekcija za prikaz sike ako je ima -->
		<% if(typeof picture != 'undefined') { %>
			<section class="content-div">
		  	<img src= "<%=picture.id%>" class="imgtool"></img>
			</section>
		<% } %>



<% } %>
</div>
<hr>
