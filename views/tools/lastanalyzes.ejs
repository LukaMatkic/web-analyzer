<div class="content-wrapper">
	<div id="titleDiv">
			Statistic tools
	</div>

<div id="snatcherDiv">
		<p>ANALYZES HISTORY</p>
		</div>



<!--If there is error we display it -->
<% if(typeof error != 'undefined') { %>
	<div id="pageTextContent">
  <center>
	<p><%= error %></p>
	</center>
</div>
<% } %>


<!--If there is warning we display it -->
<% if(typeof info != 'undefined') { %>
	<div id="pageTextContent">
	<center>
	<p><%= info %></p>
	</center>
	</div>
<% } %>


<% if(typeof scrap != 'undefined') { %>

<section>

<div class="tbl-header">
		<table cellpadding="0" cellspacing="0" border="0">
				<thead>
						<tr>
							<th  class="smaller" id="idTh">ID</th>
							<th style="width:100px;" id="thTime">TIME</th>
							<th style="width:450px;" id="thUrl">URL</th>
							<th class="smaller">STATUS</th>
							<th style="width:100px; id="thDate"">DATE</th>
							<th class="smaller">HTTP</th>
							<th class="smaller">HTML</th>
							<th class="smaller">HEAD</th>
							<th class="smaller">CHIL</th>
							<th class="smaller">IMAG</th>
							<th class="smaller">YOUR</th>
							<th class="smaller">PRIV</th>
							<th style="width:70px; id="thView"">VIEW</th>
						</tr>
				</thead>
		</table>
	</div>
<div class="tbl-content" id="historyTable">
		<table>
				<tbody>
				<% for(var i=0;i<scrap.length;i++) {%>
					<tr>
							<td  class="smaller"><%= scrap[i].id %></td>
							<td  style="width:100px;"><%= scrap[i].time %></td>
							<td style="width:460px;"><%= scrap[i].url %></td>
							<td class="smaller"><%= scrap[i].status %></td>
							<td style="width:100px;"><%= scrap[i].date.getDate() + '.' + parseInt(scrap[i].date.getMonth()+1) + '.' + scrap[i].date.getFullYear() + '.' %></td>

							<td class="smaller">
								<% if(https[i] == true) { %>
									<img src= "/imgs/true.png" class="icon"></img>
								<% } else { %>
									<img src= "/imgs/false.png" class="icon"></img>
								<% } %>
							</td>

							<td class="smaller">
								<% if(htmls[i] == true) { %>
									<img src= "/imgs/true.png" class="icon"></img>
								<% } else { %>
									<img src= "/imgs/false.png" class="icon"></img>
								<% } %>
							</td>

							<td class="smaller">
								<% if(heading[i] == true) { %>
									<img src= "/imgs/true.png" class="icon"></img>
								<% } else { %>
									<img src= "/imgs/false.png" class="icon"></img>
								<% } %>
							</td>

							<td class="smaller">
								<% if(childs[i] == true) { %>
									<img src= "/imgs/true.png" class="icon"></img>
								<% } else { %>
									<img src= "/imgs/false.png" class="icon"></img>
								<% } %>
							</td>

							<td class="smaller">
								<% if(picture[i] == true) { %>
									<img src= "/imgs/true.png" class="icon"></img>
								<% } else { %>
									<img src= "/imgs/false.png" class="icon"></img>
								<% } %>
							</td>

							<td class="smaller">
								<% if(typeof user == 'undefined') { %>
									<img src= "/imgs/false.png" class="icon"></img>
								<% } else { %>
									<% if(scrap[i].id_user == user.id) { %>
										<img src= "/imgs/true.png" class="icon"></img>
									<% } else { %>
										<img src= "/imgs/false.png" class="icon"></img>
									<% } %>
								<% } %>
							</td>

							<td class="smaller">
								<% if(scrap[i].private == true) { %>
									<img src= "/imgs/true.png" class="icon"></img>
								<% } else { %>
									<img src= "/imgs/false.png" class="icon"></img>
								<% } %>
							</td>

							<td style="width:70px;">
								<form method="POST" action="/sitedata">
									<button type="submit" name="enterid" value="<%=scrap[i].id%>" id="viewBtn">View</button>
								</form>
							</td>
					</tr>
				<%}%>
				</tbody>

		</table>
</div>

</section>


<% } %>
</div>

<div id="arrow">
<div class="arrow arrow-1"></div>
<div class="arrow arrow-2"></div>
</div>

</div>

<hr>
